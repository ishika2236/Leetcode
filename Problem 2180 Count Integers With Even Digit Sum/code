class Solution {
public:
    int countEven(int num) {
        int count=0;
        for(int i=1;i<=num;i++){
            int ele=i;
            int sum=0;
            while(ele>0){
                sum+=ele%10;
                ele/=10;
                
            }
            if(sum%2==0){
                count++;
            }

        }
        return count;
        
    }
};